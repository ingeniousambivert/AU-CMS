<!DOCTYPE html>
<html lang="en">

<head>
   <% include ./head %>
</head>

<body class="grey lighten-3">

  <!--Main Navigation-->
  <header>
 <% include ./header %>
  </header>
  <!--Main Navigation-->

  <!--Main layout-->
  <main class="pt-5 mx-lg-5">
    <div class="container-fluid mt-5">

    <section id="former-events" class="">
      
       <div class="jumbotron">
        <h2 class="display-4 text-center">Former Events</h2>
        <ul class="list-group mt-4 mb-4">
         <% formerEvents.forEach(function(item){ %>
        <li class="list-group-item"><%= item.title %></li>
         <% }) %>
        </ul>
        <a class="btn btn-primary " href="#" role="button">Details</a>
      </div>

    </section>

     <section id="upcoming-events" class="">
      
       <div class="jumbotron">
        <h2 class="display-4 text-center">Upcoming Events</h2>
       <ul class="list-group mt-4 mb-4">
        <% upcomingEvents.forEach(function(item){ %>
        <li class="list-group-item"><%= item.title %></li>
         <% }) %>
        </ul>
        <a class="btn btn-primary " href="#" role="button">Details</a>
      </div>

    </section>


       <section id="industrial-visits" class="">
      
       <div class="jumbotron">
        <h2 class="display-4 text-center">Industrial Visits</h2>
        <ul class="list-group mt-4 mb-4">
        <% visits.forEach(function(item){ %>
        <li class="list-group-item"><%= item.title %></li>
         <% }) %>
        </ul>
        <a class="btn btn-primary " href="#" role="button">Details</a>
      </div>

    </section>

    </div>
  </main>
  <!--Main layout-->

  <!--Footer-->
  <footer class="text-center font-small ">
  <% include ./footer %>
  </footer>
  <!--/.Footer-->

  <!-- SCRIPTS -->

    <!--  JavaScript -->
    <script type="text/javascript">
      let date = new Date();
      document.getElementById("copyrightDate").innerHTML = date.getFullYear();
    </script>
    <!-- JavaScript -->

  <!-- JQuery -->
  <script type="text/javascript" src="../../admin/js/jquery-3.4.1.min.js"></script>
  <!-- Bootstrap tooltips -->
  <script type="text/javascript" src="../../admin/js/popper.min.js"></script>
  <!-- Bootstrap core JavaScript -->
  <script type="text/javascript" src="../../admin/js/bootstrap.min.js"></script>
  <!-- MDB core JavaScript -->
  <script type="text/javascript" src="../../admin/js/mdb.min.js"></script>

</body>

</html>
